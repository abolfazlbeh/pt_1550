{
  "tasks": {
    "dev": "deno run --allow-net --allow-read --allow-env --allow-sys npm:vite",
    "build": "deno run --allow-net --allow-read --allow-write --allow-env --allow-sys npm:typescript/tsc -b && deno run --allow-net --allow-read --allow-write --allow-env --allow-sys npm:vite build",
    "lint": "deno run --allow-net --allow-read --allow-write --allow-env --allow-sys npm:eslint .",
    "preview": "deno run --allow-net --allow-read --allow-env --allow-sys npm:vite preview"
  },
  "imports": {
    "react": "npm:react@^19.1.1",
    "react-dom": "npm:react-dom@^19.1.1",
    "react/": "npm:react/",
    "react-dom/": "npm:react-dom/",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^5.0.3",
    "vite": "npm:vite@^7.1.7"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "nodeModulesDir": "auto",
  "lock": true
}
